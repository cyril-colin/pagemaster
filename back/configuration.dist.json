{
  "port": 3000,
  "host": "localhost",
  "apiPrefix": "/api",
  "cors": {
    "allowedOrigins": [
      "*"
    ],
    "allowedMethods": [
      "*"
    ]
  },
  "database": {
    "mongodb": {
      "url": "mongodb://pagemaster-app:pagemaster-app-password@localhost:27017/pagemaster",
      "dbName": "pagemaster"
    }
  },
  "downloadPaths": {
    "films": "/data/films",
    "series": "/data/series"
  },
  "bots": {
    "discord": {
      "http": {
        "baseUrl": "https://discord.com/api/v10",
        "appId": "changeme",
        "token": "changeme"
      },
      "bot": {
        "publicKey": "changeme"
      }
    }
  },
  "trackers": {
    "sharewood": {
      "tracker": {
        "categoriesMapper": {
          "films": [9, 11, 13],
          "series": [10, 12, 14]
        },
        "torrentPageBaseUrl": "https://www.sharewood.tv/torrents/"
      },
      "http": {
        "baseUrl": "https://sharewood.tv/api",
        "token": "changeme"
      },
      "downloadPath": "./downloads/"
    }
  },
  "torrentClient": {
    "deluge": {
      "baseUrl": "https://changeme/json",
      "password": "changeme"
    }
  },
  "logger": {
    "level": "debug",
    "transports": [
      {
        "type": "console"
      },
      {
        "type": "file",
        "filename": "logger.log",
        "logLevel": "error"
      }
    ]
  },
  "security": {
    "admin": {
      "username": "admin",
      "password": "admin"
    },
    "jwt": {
      "secret": "changeme"
    }
  }
}