{
  "port": 8080,
  "host": "localhost",
  "apiPrefix": "/api",
  "staticFilesPath": "./front",
  "cors": {
    "allowedOrigins": [
      "*"
    ],
    "allowedMethods": [
      "*"
    ]
  },
  "database": {
    "mongodb": {
      "url": "mongodb://pagemaster-app:pagemaster-app-password@localhost:27017/pagemaster",
      "dbName": "pagemaster"
    }
  },
  "logger": {
    "level": "debug",
    "transports": [
      {
        "type": "console"
      },
      {
        "type": "file",
        "filename": "logger.log",
        "logLevel": "error"
      }
    ]
  }
}